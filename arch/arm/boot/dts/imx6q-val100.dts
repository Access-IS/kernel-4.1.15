        pinctrl_val_wifi_wlreg: VAL-wifi-wlreg {
			fsl,pins = <
//				MX6QDL_PAD_SD3_DAT6__GPIO6_IO18 	0x1b0b1
				MX6QDL_PAD_DISP0_DAT9__GPIO4_IO30	0x30
