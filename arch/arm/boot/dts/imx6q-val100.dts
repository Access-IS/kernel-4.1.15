        pinctrl_val_wifi: VAL-wifi {
			fsl,pins = <
				MX6QDL_PAD_NANDF_CS2__GPIO6_IO15	0x1b030
//				MX6QDL_PAD_KEY_COL4__GPIO4_IO14 	0x1b0b1
			>;

